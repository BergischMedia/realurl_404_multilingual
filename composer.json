{
  "name": "WapplerSystems/Realurl404Multilingual",
  "type": "typo3-cms-extension",
  "description": "Shows the defined error page of the given language if the requested page or file could not be found",
  "homepage": "http://typo3.org",
  "license": [
    "GPL-2.0+"
  ],
  "authors": [
    {
      "name": "Sven Wappler",
      "role": "Developer"
    },
    {
      "name": "Jeffrey Nellissen",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": ">=7.6.1,<8.0"
  },
  "autoload": {
    "psr-4": {
      "WapplerSystems\\Realurl404Multilingual\\": "Classes"
    }
  },
  "replace": {
    "Realurl404Multilingual": "self.version",
    "typo3-ter/Realurl404Multilingual": "self.version"
  }
}